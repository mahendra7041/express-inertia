{
  "name": "svelte",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "vite build --outDir build/client",
    "build:ssr": "vite build --outDir build/ssr --ssr src/ssr.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "cross-env": "^10.0.0",
    "svelte": "^5.38.1",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@inertiajs/svelte": "^2.1.2",
    "express": "^5.1.0",
    "express-inertia": "^1.3.1",
    "express-session": "^1.18.2"
  }
}
