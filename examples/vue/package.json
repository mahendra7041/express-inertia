{
  "name": "express-inertia-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "serve": "cross-env NODE_ENV=production node server.js",
    "build": "npm run build:client && npm run build:ssr",
    "build:client": "vite build --outDir build/client --ssrManifest",
    "build:ssr": "vite build --outDir build/ssr --ssr src/ssr.js"
  },
  "dependencies": {
    "@inertiajs/vue3": "^2.1.2",
    "express": "^5.1.0",
    "express-inertia": "github:mahendra7041/express-inertia",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "cross-env": "^10.0.0",
    "vite": "^7.1.2"
  }
}
